        "    if v != 'No consta':\n",
        "      number = int(births[k].get('Number'))\n",
        "      if v in dic_natalidad:\n",
        "        suma_ant = dic_natalidad[v]\n",
        "        dic_natalidad[v] = number + suma_ant\n",
        "      else:\n",
        "        dic_natalidad[v] = number\n",
        "        \n",
        "natalidad_por_distrito()\n",
